@model IEnumerable<OdeToFood.Models.Restaurant>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>
        @foreach (var restaurants in Model)
        {
            <tr>
                <td>@restaurants.Id</td>
                <td>@restaurants.Name</td>
            </tr>
        }

    </table>
</body>
</html>